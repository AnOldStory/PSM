{
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.suggestSelection": "first",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.fontFamily": "'D2Coding', Consolas",
  "files.exclude": {
    ".git/": true,
    "node_modules/": true
  },
  "search.exclude": {
    ".git/": true
  },
  "code-runner.executorMap": {
    "c": "clear && $workspaceRoot/psm.sh -f $fullFileName",
    "cpp": "clear && $workspaceRoot/psm.sh -f $fullFileName",
    "java": "clear && $workspaceRoot/psm.sh -f $fullFileName"
  },
  "code-runner.executorMapByGlob": {},
  "code-runner.executorMapByFileExtension": {},
  "code-runner.customCommand": "clear && $workspaceRoot/psm.sh -v -f $fullFileName",
  "code-runner.clearPreviousOutput": true,
  "code-runner.showExecutionMessage": true,
  "code-runner.preserveFocus": true,
  "code-runner.runInTerminal": true,
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.defaultProfile.windows": "Ubuntu-24.04 (WSL)",
  "breadcrumbs.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0 }",
  "files.associations": {
    "string": "cpp",
    "initializer_list": "cpp",
    "*.tcc": "cpp",
    "deque": "cpp",
    "list": "cpp",
    "vector": "cpp",
    "iostream": "cpp",
    "ostream": "cpp"
  },
  "C_Cpp.default.compilerPath": "C:\\MinGW\\bin\\g++.exe"
  // "files.associations": {
  //   "ostream": "cpp",
  //   "iostream": "cpp",
  //   "bitset": "cpp"
  // }
}
